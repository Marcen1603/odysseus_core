<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="de.uniol.inf.is.odysseus.benchmarker.runner.OdysseusBenchmarkRunner">
         </run>
      </application>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="de.uniol.inf.is.odysseus.benchmarker.runner.application"
            name="Benchmark Runner">
         <property
               name="appName"
               value="Benchmark Runner">
         </property>
      </product>
   </extension>

</plugin>
