<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="de.uniol.inf.is.winddatagenerator.WDGApplication">
         </run>
      </application>
   </extension>
   <extension
         id="WindDataGenerator"
         point="org.eclipse.core.runtime.products">
      <product  name="WindDataGenerator"
            application="de.uniol.inf.is.WindDataGenerator.application"
            >
         <property
               name="appName"
               value="WindDataGenerator"
               >
         </property>
      </product>
   </extension>
</plugin>
