<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            name="windSCADA"
            class="windscadaanwendung.Perspective"
            id="windscadanwendung.perspective">
      </perspective>
   </extension>
   <extension
         id="windSCADA"
         point="org.eclipse.core.runtime.products">
      <product
            application="de.uniol.inf.is.odysseus.client.starter.OdysseusStudioApplication"
            name="windSCADA">
         <property
               name="appName"
               value="windSCADA">
         </property>
      </product>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="windscadaanwendung.category"
            name="windSCADA">
      </category>
      <view
            category="windscadaanwendung.category"
            class="windscadaanwendung.views.DetailView"
            id="windscadaanwendung.views.DetailView"
            name="Windkraftanlage">
      </view>
      <view
            category="windscadaanwendung.category"
            class="windscadaanwendung.views.ListView"
            id="windacadaanwendung.ListView"
            name="ListView"
            restorable="true">
      </view>
      <view
            category="windscadaanwendung.category"
            class="windscadaanwendung.views.UebersichtView"
            id="windscadaanwendung.views.UebersichtView"
            name="Uebersicht"
            restorable="true">
      </view>
      <view
            category="windscadaanwendung.category"
            class="windscadaanwendung.views.DPVTest"
            id="windscadaanwendung.views.DPVTest"
            name="DPVTest"
            restorable="true">
      </view>
      <view
            category="windscadaanwendung.category"
            class="windscadaanwendung.views.MapView"
            id="windscadaanwendung.views.MapView"
            name="Karte"
            restorable="true">
      </view>
   </extension>
</plugin>
