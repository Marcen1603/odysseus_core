#PARSER PQL
#RUNQUERY
Stream1 = Stream({SOURCE='System.P0:4155'})

PROJECT2 = PROJECT({ATTRIBUTES=['P0:4155.wind_speed']}, 0:Stream1)

TIMEWINDOW3 = TIMEWINDOW({SIZE=[5, 'SECONDS'], SLIDE=[5, 'SECONDS']}, 0:PROJECT2)

AGGREGATE4 = AGGREGATE({AGGREGATIONS=[['AVG','wind_speed','AVG_wind_speed','Double']]}, 0:TIMEWINDOW3)

P0:4155:wind_speed ::= CACHE({MAXELEMENTS=100}, 0:AGGREGATE4)