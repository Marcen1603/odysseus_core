CREATE STREAM flapSimComplete(
	timestamp STARTTIMESTAMP,
	windSpeedHH DOUBLE,
	windDir DOUBLE,
	assPowerWT1 DOUBLE,
	assPowerWT2 DOUBLE,
	assPowerC DOUBLE,
	realPowerWT1 DOUBLE,
	realPowerWT2 DOUBLE,
	realPowerCFree DOUBLE,
	realPowerCRisoe DOUBLE,
	windSpeedWT1 DOUBLE,
	windSpeedWT2 DOUBLE
) SOCKET localhost : 60101

CREATE STREAM meteoSim(
	timestamp STARTTIMESTAMP,
	pressure DOUBLE,
	temperature DOUBLE
) SOCKET localhost : 60102