#PARSER PQL
#RUNQUERY
Stream1 = Stream({SOURCE='System.P§windfarmid§:§wkaid§'})
PROJECT2 = PROJECT({ATTRIBUTES=['P§windfarmid§:§wkaid§.corrected_score', 'P§windfarmid§:§wkaid§.wind_speed']}, 0:Stream1)
TIMEWINDOW3 = TIMEWINDOW({SIZE=[5, 'SECONDS'], SLIDE=[5, 'SECONDS']}, 0:PROJECT2)
AGGREGATE4 = AGGREGATE({DRAINATDONE='false', DRAINATCLOSE='false', OUTPUTPA='false', DRAIN='false', FASTGROUPING='false', AGGREGATIONS=[['AVG','corrected_score','AVG_corrected_score','Double'], ['AVG','wind_speed','AVG_wind_speed','Double']]}, 0:TIMEWINDOW3)
P§windfarmid§:§wkaid§:corrected_score_wind_speed ::= CACHE({MAXELEMENTS=100}, 0:AGGREGATE4)
