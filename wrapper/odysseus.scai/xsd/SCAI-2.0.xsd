<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://xml.offis.de/schema/SCAI-2.0" xmlns:tns="http://xml.offis.de/schema/SCAI-2.0" elementFormDefault="qualified">
    <xsd:element name="SCAI">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="Identification" minOccurs="0">
                    <xsd:complexType>
                        <xsd:choice>
                            <xsd:element name="SecurityToken" type="xsd:string"></xsd:element>
                            <xsd:element name="LoginIdentification">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="Username" type="xsd:string"/>
                                        <xsd:element name="Password" type="xsd:base64Binary"></xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:choice>
                    </xsd:complexType>
                </xsd:element>
                <xsd:element name="Payload">
                    <xsd:complexType>
                        <xsd:choice>
                            <!--<xsd:element name="SensorData" maxOccurs="unbounded" type="tns:SensorData" minOccurs="1"/>-->
                            <xsd:element name="Measurements" >
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="DataStream" type="tns:SensorDataDescription" maxOccurs="unbounded"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element name="ControlData" minOccurs="1">
                                <xsd:complexType>
                                    <xsd:choice>
                                        <xsd:element name="AccessControl" minOccurs="1" maxOccurs="unbounded">
                                            <xsd:complexType>
                                                <xsd:choice>
                                                    <xsd:element name="authenticateUser" minOccurs="1">
                                                        <xsd:complexType>
                                                            <xsd:sequence>
                                                                <xsd:element name="username" type="xsd:string"/>
                                                                <xsd:element name="password" type="xsd:base64Binary"/>
                                                            </xsd:sequence>
                                                            <xsd:attribute name="OperationID" type="xsd:string"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="createUser" minOccurs="1" maxOccurs="1">
                                                        <xsd:complexType>
                                                            <xsd:sequence>
                                                                <xsd:element name="User" type="tns:UserDescription"/>
                                                            </xsd:sequence>
                                                            <xsd:attribute name="OperationID" type="xsd:string"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="destroySession" minOccurs="1">
                                                        <xsd:complexType>
                                                            <xsd:sequence>
                                                                <xsd:element name="sessionID" minOccurs="0" type="xsd:string"/>
                                                            </xsd:sequence>
                                                            <xsd:attribute name="OperationID" type="xsd:string"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="removeUser" minOccurs="1" maxOccurs="1">
                                                        <xsd:complexType>
                                                            <xsd:sequence>
                                                                <xsd:element name="User" type="tns:Reference"/>
                                                            </xsd:sequence>
                                                            <xsd:attribute name="OperationID" type="xsd:string"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="getUserData" minOccurs="1" maxOccurs="1">
                                                        <xsd:complexType>
                                                            <xsd:sequence minOccurs="1">
                                                                <xsd:element name="User" type="tns:Reference"/>
                                                            </xsd:sequence>
                                                            <xsd:attribute name="OperationID" type="xsd:string" use="required"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="grantSensorAccess">
                                                        <xsd:complexType>
                                                            <xsd:sequence>
                                                                <xsd:element name="Sensor" type="tns:SensorReference" form="qualified"/>
                                                                <xsd:element name="Accessor" maxOccurs="unbounded" type="tns:AccessorDescription"></xsd:element>
                                                            </xsd:sequence>
                                                            <xsd:attribute name="OperationID" type="xsd:string" use="optional"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="listAllSensorAccessors">
                                                        <xsd:complexType>
                                                            <xsd:sequence>
                                                                <xsd:element name="Sensor" type="tns:SensorReference"/>
                                                            </xsd:sequence>
                                                            <xsd:attribute name="OperationID" type="xsd:string" use="required"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="updateUser" minOccurs="1" maxOccurs="1">
                                                        <xsd:complexType>
                                                            <xsd:sequence>
                                                                <xsd:element name="User" type="tns:Reference"/>
                                                                <xsd:element name="NewUser" type="tns:UserDescription"/>
                                                            </xsd:sequence>
                                                            <xsd:attribute name="OperationID" type="xsd:string"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="withdrawSensorAccess">
                                                        <xsd:complexType>
                                                            <xsd:sequence>
                                                                <xsd:element name="Sensor" type="tns:SensorReference"/>
                                                                <xsd:element name="Accessor" maxOccurs="unbounded" type="tns:AccessorDescription"></xsd:element>
                                                            </xsd:sequence>
                                                            <xsd:attribute name="OperationID" type="xsd:string"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                 </xsd:choice>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="SensorControl" minOccurs="1" maxOccurs="unbounded">
                                            <xsd:complexType>
                                                <xsd:choice>
                                                    <xsd:element name="configureSensor" minOccurs="1" maxOccurs="1">
                                                        <xsd:complexType>
                                                            <xsd:sequence>
                                                                <xsd:element name="Sensor" type="tns:SensorReference"/>
                                                                <xsd:element name="ConfigurationValue" type="tns:ConfigurationValueDescription"></xsd:element>
                                                            </xsd:sequence>
                                                            <xsd:attribute name="OperationID" type="xsd:string"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="getConfiguration" minOccurs="1" maxOccurs="1">
                                                        <xsd:complexType>
                                                            <xsd:sequence>
                                                                <xsd:element name="Sensor" type="tns:SensorReference"/>
                                                            </xsd:sequence>
                                                            <xsd:attribute name="OperationID" type="xsd:string" use="required"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="getSupportedModules" minOccurs="1" maxOccurs="1">
                                                        <xsd:complexType>
                                                            <xsd:sequence minOccurs="1">
                                                                <xsd:element name="Sensor" type="tns:SensorReference"/>
                                                            </xsd:sequence>
                                                            <xsd:attribute name="OperationID" type="xsd:string" use="required"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="getValue" minOccurs="1">
                                                        <xsd:complexType>
                                                            <xsd:sequence>
                                                                <xsd:element name="Sensor" type="tns:SensorReference"/>
                                                            </xsd:sequence>
                                                            <xsd:attribute name="OperationID" type="xsd:string" use="required"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="unsubscribeDatastream" minOccurs="1" maxOccurs="1">
                                                        <xsd:complexType>
                                                            <xsd:sequence>
                                                                <xsd:element name="Sensor" type="tns:SensorReference"/>
                                                            </xsd:sequence>
                                                            <xsd:attribute name="OperationID" type="xsd:string"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="startSensor" maxOccurs="1" minOccurs="1">
                                                        <xsd:complexType>
                                                            <xsd:sequence>
                                                                <xsd:element name="Sensor" type="tns:SensorReference"/>
                                                            </xsd:sequence>
                                                            <xsd:attribute name="OperationID" type="xsd:string"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="stopSensor" minOccurs="1" maxOccurs="1">
                                                        <xsd:complexType>
                                                            <xsd:sequence>
                                                                <xsd:element name="Sensor" type="tns:SensorReference"/>
                                                            </xsd:sequence>
                                                            <xsd:attribute name="OperationID" type="xsd:string"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="subscribeDatastream" minOccurs="1" maxOccurs="1">
                                                        <xsd:complexType>
                                                            <xsd:sequence>
                                                                <xsd:element name="Sensor" type="tns:SensorReference"/>
                                                            </xsd:sequence>
                                                            <xsd:attribute name="OperationID" type="xsd:string"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                </xsd:choice>
                                            </xsd:complexType>
                                        </xsd:element>
                                        <xsd:element name="SensorRegistryControl" maxOccurs="unbounded" minOccurs="1">
                                            <xsd:complexType>
                                                <xsd:choice>
                                                    <xsd:element name="addChildCategoryToSensorCategory">
                                                        <xsd:complexType>
                                                            <xsd:sequence>
                                                                <xsd:element name="SensorCategory" type="tns:Reference"></xsd:element>
                                                                <xsd:element name="ParentSensorCategory" type="tns:Reference"></xsd:element>
                                                            </xsd:sequence>
                                                            <xsd:attribute name="OperationID" type="xsd:string"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="addDataElementToDataStreamType">
                                                        <xsd:complexType>
                                                            <xsd:sequence>
                                                                <xsd:element name="DataElement" type="tns:Reference"></xsd:element>
                                                                <xsd:element name="DataStreamType" type="tns:Reference"></xsd:element>
                                                            </xsd:sequence>
                                                            <xsd:attribute name="OperationID" type="xsd:string"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="addSensorCategoryToSensorDomain">
                                                        <xsd:complexType>
                                                            <xsd:sequence>
                                                                <xsd:element name="SensorCategory" type="tns:Reference"></xsd:element>
                                                                <xsd:element name="SensorDomain" type="tns:Reference"></xsd:element>
                                                            </xsd:sequence>
                                                            <xsd:attribute name="OperationID" type="xsd:string"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="addSensorTypeToSensorCategory">
                                                        <xsd:complexType>
                                                            <xsd:sequence>
                                                                <xsd:element name="SensorType" type="tns:Reference"></xsd:element>
                                                                <xsd:element name="SensorCategory" type="tns:Reference"></xsd:element>
                                                            </xsd:sequence>
                                                            <xsd:attribute name="OperationID" type="xsd:string"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="addSensorTypeToSensorDomain">
                                                        <xsd:complexType>
                                                            <xsd:sequence>
                                                                <xsd:element name="SensorType" type="tns:Reference"></xsd:element>
                                                                <xsd:element name="SensorDomain" type="tns:Reference"></xsd:element>
                                                            </xsd:sequence>
                                                            <xsd:attribute name="OperationID" type="xsd:string"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="createConfigurationParameter" maxOccurs="1" minOccurs="1">
                                                        <xsd:complexType>
                                                            <xsd:sequence>
                                                                <xsd:element name="ConfigurationParameter" type="tns:ConfigurationParameterDescription"/>
                                                            </xsd:sequence>
                                                            <xsd:attribute name="OperationID" type="xsd:string"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="createDataElement" minOccurs="1" maxOccurs="1">
                                                        <xsd:complexType>
                                                            <xsd:sequence>
                                                                <xsd:element name="DataElement" type="tns:DataElementDescription"/>
                                                            </xsd:sequence>
                                                            <xsd:attribute name="OperationID" type="xsd:string"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="createDataStreamType" maxOccurs="1" minOccurs="1">
                                                        <xsd:complexType>
                                                            <xsd:sequence>
                                                                <xsd:element name="DataStreamType" type="tns:DataStreamTypeDescription"/>
                                                            </xsd:sequence>
                                                            <xsd:attribute name="OperationID" type="xsd:string"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="createDataType" maxOccurs="1" minOccurs="1">
                                                        <xsd:complexType>
                                                            <xsd:sequence>
                                                                <xsd:element name="DataType" type="tns:DataTypeDescription"/>
                                                            </xsd:sequence>
                                                            <xsd:attribute name="OperationID" type="xsd:string"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="createOperator" minOccurs="1" maxOccurs="1">
                                                        <xsd:complexType>
                                                            <xsd:sequence>
                                                                <xsd:element name="OperatorGroup" type="tns:Reference"/>
                                                                <xsd:element name="Operator" type="tns:OperatorDescription" maxOccurs="1"/>
                                                            </xsd:sequence>
                                                            <xsd:attribute name="OperationID" type="xsd:string"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="createOperatorGroup" minOccurs="1" maxOccurs="1">
                                                        <xsd:complexType>
                                                            <xsd:sequence>
                                                                <xsd:element name="OperatorGroup" type="tns:OperatorGroupDescription"/>
                                                            </xsd:sequence>
                                                            <xsd:attribute name="OperationID" type="xsd:string"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="createOperatorType" minOccurs="1" maxOccurs="1">
                                                        <xsd:complexType>
                                                            <xsd:sequence>
                                                                <xsd:element name="OperatorType" type="tns:OperatorTypeDescription" maxOccurs="1"/>
                                                            </xsd:sequence>
                                                            <xsd:attribute name="OperationID" type="xsd:string"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="createSensor" maxOccurs="1" minOccurs="1">
                                                        <xsd:complexType>
                                                            <xsd:sequence minOccurs="1">
                                                                <xsd:element name="Sensor" type="tns:SensorDescription"/>
                                                            </xsd:sequence>
                                                            <xsd:attribute name="OperationID" type="xsd:string"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="createSensorCategory" maxOccurs="1" minOccurs="1">
                                                        <xsd:complexType>
                                                            <xsd:sequence>
                                                                <xsd:element name="SensorCategory" type="tns:SensorCategoryDescription"/>
                                                            </xsd:sequence>
                                                            <xsd:attribute name="OperationID" type="xsd:string"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="createSensorDomain" maxOccurs="1" minOccurs="1">
                                                        <xsd:complexType>
                                                            <xsd:sequence>
                                                                <xsd:element name="SensorDomain" type="tns:SensorDomainDescription"/>
                                                            </xsd:sequence>
                                                            <xsd:attribute name="OperationID" type="xsd:string"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="createSensorType" maxOccurs="1" minOccurs="1">
                                                        <xsd:complexType>
                                                            <xsd:sequence minOccurs="1">
                                                                <xsd:element name="SensorType" type="tns:SensorTypeDescription"/>
                                                            </xsd:sequence>
                                                            <xsd:attribute name="OperationID" type="xsd:string"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="deployOperatorGroup">
                                                        <xsd:complexType>
                                                            <xsd:sequence>
                                                                <xsd:element name="OperatorGroup" type="tns:Reference"/>
                                                                <xsd:element name="ProcessingUnit" minOccurs="0" type="tns:Reference"/>
                                                            </xsd:sequence>
                                                            <xsd:attribute name="OperationID" type="xsd:string"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="getConfigurationParameter" minOccurs="1" maxOccurs="1">
                                                        <xsd:complexType>
                                                            <xsd:sequence minOccurs="1">
                                                                <xsd:element name="ConfigurationParameter" type="tns:Reference"/>
                                                            </xsd:sequence>
                                                            <xsd:attribute name="OperationID" type="xsd:string" use="required"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="getDataElement" minOccurs="1" maxOccurs="1">
                                                        <xsd:complexType>
                                                            <xsd:sequence minOccurs="1">
                                                                <xsd:element name="DataElement" type="tns:Reference"/>
                                                            </xsd:sequence>
                                                            <xsd:attribute name="OperationID" type="xsd:string" use="required"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="getDataStreamType" minOccurs="1" maxOccurs="1">
                                                        <xsd:complexType>
                                                            <xsd:sequence minOccurs="1">
                                                                <xsd:element name="DataStreamType" type="tns:Reference"/>
                                                            </xsd:sequence>
                                                            <xsd:attribute name="OperationID" type="xsd:string" use="required"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="getDataType" minOccurs="1" maxOccurs="1">
                                                        <xsd:complexType>
                                                            <xsd:sequence minOccurs="1">
                                                                <xsd:element name="DataType" type="tns:Reference"/>
                                                            </xsd:sequence>
                                                            <xsd:attribute name="OperationID" type="xsd:string" use="required"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="getOperator" minOccurs="1" maxOccurs="1">
                                                        <xsd:complexType>
                                                            <xsd:sequence minOccurs="1">
                                                                <xsd:element name="Operator" type="tns:Reference"/>
                                                                <xsd:element name="OperatorGroup" type="tns:Reference"/>
                                                            </xsd:sequence>
                                                            <xsd:attribute name="OperationID" type="xsd:string" use="required"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="getOperatorGroup" minOccurs="1" maxOccurs="1">
                                                        <xsd:complexType>
                                                            <xsd:sequence minOccurs="1">
                                                                <xsd:element name="OperatorGroup" type="tns:Reference"/>
                                                            </xsd:sequence>
                                                            <xsd:attribute name="OperationID" type="xsd:string" use="required"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="getOperatorGroupStatus" minOccurs="1" maxOccurs="1">
                                                        <xsd:complexType>
                                                            <xsd:sequence minOccurs="1">
                                                                <xsd:element name="OperatorGroup" type="tns:Reference"/>
                                                            </xsd:sequence>
                                                            <xsd:attribute name="OperationID" type="xsd:string" use="required"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="getOperatorType" minOccurs="1" maxOccurs="1">
                                                        <xsd:complexType>
                                                            <xsd:sequence minOccurs="1">
                                                                <xsd:element name="OperatorType" type="tns:Reference"/>
                                                            </xsd:sequence>
                                                            <xsd:attribute name="OperationID" type="xsd:string" use="required"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="getProcessingUnit">
                                                        <xsd:complexType>
                                                            <xsd:sequence>
                                                                <xsd:element name="OperatorGroup" type="tns:Reference"/>
                                                            </xsd:sequence>
                                                            <xsd:attribute name="OperationID" type="xsd:string" use="required"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="getSensor" minOccurs="1" maxOccurs="1">
                                                        <xsd:complexType>
                                                            <xsd:sequence minOccurs="1">
                                                                <xsd:element name="Sensor" type="tns:SensorReference"/>
                                                            </xsd:sequence>
                                                            <xsd:attribute name="OperationID" type="xsd:string" use="required"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="getSensorCategory" minOccurs="1" maxOccurs="1">
                                                        <xsd:complexType>
                                                            <xsd:sequence minOccurs="1">
                                                                <xsd:element name="SensorCategory" type="tns:Reference"/>
                                                            </xsd:sequence>
                                                            <xsd:attribute name="OperationID" type="xsd:string" use="required"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="getSensorDomain" minOccurs="1" maxOccurs="1">
                                                        <xsd:complexType>
                                                            <xsd:sequence minOccurs="1">
                                                                <xsd:element name="SensorDomain" type="tns:Reference"/>
                                                            </xsd:sequence>
                                                            <xsd:attribute name="OperationID" type="xsd:string" use="required"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="getSensorType" minOccurs="1" maxOccurs="1">
                                                        <xsd:complexType>
                                                            <xsd:sequence minOccurs="1">
                                                                <xsd:element name="SensorType" type="tns:Reference"/>
                                                            </xsd:sequence>
                                                            <xsd:attribute name="OperationID" type="xsd:string" use="required"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="linkOperators">
                                                        <xsd:complexType>
                                                            <xsd:sequence>
                                                                <xsd:element name="OperatorGroup" type="tns:Reference"/>
                                                                <xsd:element name="OperatorLink" type="tns:OperatorLinkDescription"></xsd:element>
                                                            </xsd:sequence>
                                                            <xsd:attribute name="OperationID" type="xsd:string"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="listAllConfigurationParameters" minOccurs="1">
                                                        <xsd:complexType>
                                                            <xsd:attribute name="OperationID" type="xsd:string" use="required"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="listAllDataElements" minOccurs="1">
                                                        <xsd:complexType>
                                                            <xsd:attribute name="OperationID" type="xsd:string" use="required"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="listAllDataStreamTypes" minOccurs="1">
                                                        <xsd:complexType>
                                                            <xsd:attribute name="OperationID" type="xsd:string" use="required"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="listAllDataTypes" minOccurs="1">
                                                        <xsd:complexType>
                                                            <xsd:attribute name="OperationID" type="xsd:string" use="required"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="listAllLinks">
                                                        <xsd:complexType>
                                                            <xsd:sequence minOccurs="1">
                                                                <xsd:element name="OperatorGroup" type="tns:Reference" minOccurs="1"/>
                                                            </xsd:sequence>
                                                            <xsd:attribute name="OperationID" type="xsd:string" use="required" form="unqualified"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="listAllOperators" minOccurs="1">
                                                        <xsd:complexType>
                                                            <xsd:sequence>
                                                                <xsd:element name="OperatorGroup" type="tns:Reference" minOccurs="1"/>
                                                            </xsd:sequence>
                                                            <xsd:attribute name="OperationID" type="xsd:string" use="required"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="listAllOperatorGroups" minOccurs="1">
                                                        <xsd:complexType>
                                                            <xsd:attribute name="OperationID" type="xsd:string" use="required"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="listAllOperatorTypes" minOccurs="1">
                                                        <xsd:complexType>
                                                            <xsd:attribute name="OperationID" type="xsd:string" use="required"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="listAllProcessingUnits" minOccurs="1">
                                                        <xsd:complexType>
                                                            <xsd:attribute name="OperationID" type="xsd:string" use="required"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="listAllSensors" minOccurs="1">
                                                        <xsd:complexType>
                                                            <xsd:attribute name="OperationID" type="xsd:string" use="required"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="listAllSensorCategories" minOccurs="1">
                                                        <xsd:complexType>
                                                            <xsd:attribute name="OperationID" type="xsd:string" use="required"/>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                    <xsd:element name="listAllSensorCategoriesBySensorDomain" minOccurs="1">
                                                        <xsd:complexType>
                                                            <xsd:sequence>
                                                                <xsd:element name="SensorDomain" type="tns:Reference"></xsd:element>
                                                            </xsd:sequence>
                                                            <xsd:attribute name="OperationID" type="xsd:string" use="required"/>
                                                        </xsd:complexType>
