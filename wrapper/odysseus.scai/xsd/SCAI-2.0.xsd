                        <xsd:element name="ServiceOperator" type="tns:Reference"/>
                        <xsd:element name="OutputOperator" type="tns:Reference"/>
                    </xsd:choice>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="OperatorTypeDescription">
        <xsd:sequence>
            <xsd:element name="name" type="xsd:string" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="metaType" type="xsd:string" maxOccurs="1" minOccurs="1"/>
            <xsd:element name="Property" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="key" type="xsd:string"/>
                        <xsd:element name="value" type="xsd:string"/>
                        <xsd:element name="readOnly" type="xsd:boolean"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="Description" type="xsd:string" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="PermissionDescription">
        <xsd:sequence>
            <xsd:element name="Permission">
                <xsd:complexType>
                    <xsd:choice>
                        <xsd:element name="grant" type="xsd:string"/>
                        <xsd:element name="withdraw" type="xsd:string"/>
                    </xsd:choice>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="Property" maxOccurs="unbounded" minOccurs="0">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="key" type="xsd:string"></xsd:element>
                        <xsd:element name="value" type="xsd:string"></xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="inheritable" minOccurs="0" maxOccurs="1" type="xsd:boolean"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="ProcessingUnitDescription">
        <xsd:sequence>
            <xsd:element name="name" type="xsd:string" maxOccurs="1" minOccurs="1"/>
            <xsd:element name="address" type="xsd:anyURI"/>
            <xsd:element name="version" type="xsd:double"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="Reference">
        <xsd:choice>
            <xsd:element name="name" type="xsd:string"/>
            <xsd:element name="id" type="xsd:string"/>
        </xsd:choice>
    </xsd:complexType>
    <xsd:complexType name="SensorCategoryDescription">
        <xsd:sequence>
            <xsd:element name="name" type="xsd:string"/>
            <xsd:element name="ParentSensorCategory" type="tns:Reference" minOccurs="0"/>
            <xsd:element name="SensorDomain" type="tns:Reference" maxOccurs="unbounded" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="SensorDataDescription">
        <xsd:sequence>
            <xsd:element name="timeStamp" type="xsd:dateTime"/>
            <xsd:element name="SensorName" type="xsd:string"/>
            <xsd:element name="SensorDomainName" type="xsd:string"/>
            <xsd:element name="olderValues" type="xsd:int" minOccurs="0"/>
            <xsd:element name="DataStreamElement" type="tns:DataElementValueDescription" maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="SensorDescription">
        <xsd:sequence minOccurs="1">
            <xsd:element name="name" type="xsd:string"/>
            <xsd:element name="virtual" type="xsd:boolean"/>
            <xsd:element name="SensorDomain" type="tns:Reference" minOccurs="1"/>
            <xsd:element name="SensorType" type="tns:Reference" minOccurs="1"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="SensorDomainDescription">
        <xsd:sequence minOccurs="1">
            <xsd:element name="name" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="SensorReference">
        <xsd:choice>
            <xsd:element name="SensorID" type="xsd:string"/>
            <xsd:element name="NameReference">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="SensorName" type="xsd:string"/>
                        <xsd:element name="SensorDomainName" type="xsd:string"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:choice>
    </xsd:complexType>
    <xsd:complexType name="SensorTypeDescription">
        <xsd:sequence>
            <xsd:element name="name" type="xsd:string"/>
            <xsd:element name="adapter" type="xsd:string"/>
            <xsd:element name="DataStreamType" type="tns:Reference"/>
            <xsd:element name="ConfigurationParameter" maxOccurs="unbounded" type="tns:Reference" minOccurs="0"></xsd:element>
            <xsd:element name="SensorCategory" maxOccurs="unbounded" type="tns:Reference" minOccurs="0"/>
            <xsd:element name="SensorDomain" maxOccurs="unbounded" type="tns:Reference" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="UserDescription">
        <xsd:sequence>
            <xsd:element name="username" type="xsd:string"/>
            <xsd:element name="password" type="xsd:base64Binary"/>
            <xsd:element name="Permission" maxOccurs="unbounded" minOccurs="0" type="tns:PermissionDescription"></xsd:element>
            <xsd:element name="role"  minOccurs="0" maxOccurs="unbounded" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>
</xsd:schema>
