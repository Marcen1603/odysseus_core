<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            category="Odysseus"
            class="de.uniol.inf.is.odysseus.rcp.server.views.user.UserView"
            icon="icons/users.png"
            id="de.uniol.inf.is.odysseus.rcp.views.UserView"
            name="Users"
            restorable="true">
      </view>
      <view
            category="Odysseus"
            class="de.uniol.inf.is.odysseus.rcp.server.views.sink.SinkView"
            icon="icons/sinks.png"
            id="de.uniol.inf.is.odysseus.rcp.views.SinkView"
            name="Sinks"
            restorable="true">
      </view>
      <view
            category="Odysseus"
            class="de.uniol.inf.is.odysseus.rcp.server.views.source.SourcesView"
            icon="icons/sources.png"
            id="de.uniol.inf.is.odysseus.rcp.views.SourcesView"
            name="Sources"
            restorable="true">
      </view>
      <view
            category="Odysseus"
            class="de.uniol.inf.is.odysseus.rcp.server.views.access.ProtocolsView"
            icon="icons/protocols.png"
            id="de.uniol.inf.is.odysseus.rcp.views.ProtocolsView"
            name="Protocols"
            restorable="true">
      </view>
      <view
            category="Odysseus"
            class="de.uniol.inf.is.odysseus.rcp.server.views.access.TransportsView"
            icon="icons/transports.png"
            id="de.uniol.inf.is.odysseus.rcp.views.TransportsView"
            name="Transports"
            restorable="true">
      </view>
      <view
            allowMultiple="false"
            category="Odysseus"
            class="de.uniol.inf.is.odysseus.rcp.server.views.mepfunctions.MEPFunctionsView"
            icon="icons/variable_view.gif"
            id="de.uniol.inf.is.odysseus.rcp.views.MEPFunctionsView"
            name="MEP Functions View"
            restorable="true">
      </view>
      <view
            category="Odysseus"
            class="de.uniol.inf.is.odysseus.rcp.server.views.storedprocedures.StoredProceduresView"
            icon="icons/function.png"
            id="de.uniol.inf.is.odysseus.rcp.views.StoredProceduresView"
            name="Stored Procedures"
            restorable="true">
      </view>
      <view
            category="Odysseus"
            class="de.uniol.inf.is.odysseus.rcp.server.views.opdetail.OperatorDetailView"
            icon="icons/operator_details.png"
            id="de.uniol.inf.is.odysseus.rcp.views.OperatorDetailView"
            name="Operator Detail Info"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="de.uniol.inf.is.odysseus.rcp.perspectives.QueriesPerspective">
         <view
               id="de.uniol.inf.is.odysseus.rcp.views.SinkView"
               minimized="false"
               ratio="0.25f"
               relationship="bottom"
               relative="org.eclipse.ui.navigator.ProjectExplorer">
         </view>
         <view
               id="de.uniol.inf.is.odysseus.rcp.views.SourcesView"
               minimized="false"
               ratio="0.25f"
               relationship="bottom"
               relative="de.uniol.inf.is.odysseus.rcp.views.SinkView">
         </view>
         <view
               id="de.uniol.inf.is.odysseus.rcp.views.UserView"
               minimized="false"
               ratio="0.25f"
               relationship="stack"
               relative="org.eclipse.ui.views.ContentOutline">
         </view>
         <view
               closeable="true"
               id="de.uniol.inf.is.odysseus.rcp.views.OperatorDetailView"
               minimized="false"
               moveable="true"
               ratio="0.6f"
               relationship="bottom"
               relative="org.eclipse.ui.views.ContentOutline"
               showTitle="true"
               standalone="false"
               visible="true">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="de.uniol.inf.is.odysseus.rcp.server.commands.ExpandSourcesViewCommand"
            id="de.uniol.inf.is.odysseus.rcp.commands.ExpandSourcesViewCommand"
            name="Expand Sources View">
      </command>
      <command
            defaultHandler="de.uniol.inf.is.odysseus.rcp.server.commands.CollapseSourcesViewCommand"
            id="de.uniol.inf.is.odysseus.rcp.commands.CollapseSourcesViewCommand"
            name="Collapse Sources View">
      </command>
      <command
            defaultHandler="de.uniol.inf.is.odysseus.rcp.server.commands.RefreshSourcesCommand"
            id="de.uniol.inf.is.odysseus.rcp.commands.RefreshSourcesViewCommand"
            name="Refresh Sources View">
      </command>
      <command
            defaultHandler="de.uniol.inf.is.odysseus.rcp.server.commands.RefreshSinksViewCommand"
            id="de.uniol.inf.is.odysseus.rcp.commands.RefreshSinksViewCommand"
            name="Refresh Sinks View">
      </command>
      <command
            defaultHandler="de.uniol.inf.is.odysseus.rcp.server.commands.RefreshMEPFunctionsViewCommand"
            id="de.uniol.inf.is.odysseus.rcp.commands.RefreshMEPFunctionsViewCommand"
            name="Refresh MEP Functions View">
      </command>
      <command
            defaultHandler="de.uniol.inf.is.odysseus.rcp.server.commands.SetSortingInSourcesViewCommand"
            id="de.uniol.inf.is.odysseus.rcp.commands.SetSortingInSourcesViewCommand"
            name="Sort in SourcesView">
      </command>
      <command
            defaultHandler="de.uniol.inf.is.odysseus.rcp.server.commands.DisableRefreshSourcesCommand"
            id="de.uniol.inf.is.odysseus.rcp.commands.DisableRefreshSourcesViewCommand"
            name="Disable Refreshing Sources View">
      </command>
      <command
            defaultHandler="de.uniol.inf.is.odysseus.rcp.server.commands.SelectAllFromSource"
            id="de.uniol.inf.is.odysseus.rcp.commands.SelectAllFromSource"
            name="Select all from Source">
      </command>
    </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="toolbar:de.uniol.inf.is.odysseus.rcp.views.MEPFunctionsView">
         <command
               commandId="de.uniol.inf.is.odysseus.rcp.commands.RefreshMEPFunctionsViewCommand"
               icon="icons/refresh.gif"
               label="Refresh"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="toolbar:de.uniol.inf.is.odysseus.rcp.views.SourcesView?before=DropAllSources">
         <command
               commandId="de.uniol.inf.is.odysseus.rcp.commands.SetSortingInSourcesViewCommand"
               icon="icons/sortAlpha.gif"
               label="Sort alphabetically"
               style="toggle">
         </command>
         <separator
               name="de.uniol.inf.is.odysseus.rcp.server.separator1"
               visible="true">
         </separator>
         <command
               commandId="de.uniol.inf.is.odysseus.rcp.commands.DisableRefreshSourcesViewCommand"
               icon="icons/disable-refresh.gif"
               label="Diable Refresh"
               style="toggle">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:de.uniol.inf.is.odysseus.rcp.views.SourcesView">
         <command
               commandId="de.uniol.inf.is.odysseus.rcp.commands.SelectAllFromSource"
               icon="icons/zoom.gif"
               label="Show data"
               style="push">
         </command>
      </menuContribution>
   </extension>

</plugin>
