<?xml version="1.0" encoding="ISO-8859-1"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" xmlns:sdfSchema="http://www-is.informatik.uni-oldenburg.de/~grawund/rdf/2003/04/sdf_schema.sdf#" xmlns:sdfMappings="http://www-is.informatik.uni-oldenburg.de/~grawund/rdf/2003/04/sdf_mappings.sdf#" xmlns:sdfUnits="http://www-is.informatik.uni-oldenburg.de/~grawund/rdf/2003/04/sdf_units.sdf#" xmlns:sdfDatatypes="http://www-is.informatik.uni-oldenburg.de/~grawund/rdf/2003/04/sdf_datatypes.sdf#" xmlns:sdfFunctions="http://www-is.informatik.uni-oldenburg.de/~grawund/rdf/2003/04/sdf_functions.sdf#" xml:base="http://www-is.informatik.uni-oldenburg.de/~grawund/rdf/2003/04/sdf_unit_mappings.sdf">

    <!-- ++++++++++++++++++++++++++++++++++++++++++++++++ 
         Definitionen der Funktionsnamen fuer die Euro
         Dollar Umrechnungen 
     ++++++++++++++++++++++++++++++++++++++++++++++++ -->
        
    <sdfMappings:Equivalence rdf:ID="EuroDollarMap">
        <sdfMappings:hasUnitIn rdf:resource="http://www-is.informatik.uni-oldenburg.de/~grawund/rdf/2003/04/sdf_units.sdf#Euro"/>
        <sdfMappings:hasUnitOut rdf:resource="http://www-is.informatik.uni-oldenburg.de/~grawund/rdf/2003/04/sdf_units.sdf#Dollar"/>
        <sdfMappings:usesFunction rdf:resource="http://www-is.informatik.uni-oldenburg.de/~grawund/rdf/2003/04/sdf_units.sdf#EuroDollarTrafo"/>
    </sdfMappings:Equivalence>

    <sdfFunctions:mathCalc rdf:ID="EuroDollarTrafo">      
        <sdfFunctions:hasReverse rdf:resource="#DollarEuroTrafo"/>
    </sdfFunctions:mathCalc>

    <sdfFunctions:mathCalc rdf:ID="DollarEuroTrafo">
        <sdfFunctions:hasReverse rdf:resource="#EuroDollarTrafo"/>
    </sdfFunctions:mathCalc>

</rdf:RDF>