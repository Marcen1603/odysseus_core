      <import plugin="com.google.guava"/>
      <import plugin="de.uniol.inf.is.odysseus.intervalapproach"/>
      <import plugin="de.uniol.inf.is.odysseus.mep"/>
      <import plugin="de.uniol.inf.is.odysseus.relational.base"/>
