      <import plugin="de.uniol.inf.is.odysseus.probabilistic.common" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="de.uniol.inf.is.odysseus.latency" version="1.0.0" match="greaterOrEqual"/>
   </requires>

   <plugin
         id="de.uniol.inf.is.odysseus.probabilistic"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="de.uniol.inf.is.odysseus.probabilistic.transform"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.apache.commons.math3"
         download-size="0"
         install-size="0"
         version="0.0.0"
