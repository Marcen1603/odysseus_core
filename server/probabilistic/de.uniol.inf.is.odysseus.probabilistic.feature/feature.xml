      <import plugin="org.eclipse.osgi"/>
      <import plugin="de.uniol.inf.is.odysseus.slf4j"/>
      <import plugin="de.uniol.inf.is.odysseus.probabilistic" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="de.uniol.inf.is.odysseus.relational_interval"/>
      <import plugin="de.uniol.inf.is.odysseus.ruleengine"/>
      <import plugin="de.uniol.inf.is.odysseus.transform"/>
