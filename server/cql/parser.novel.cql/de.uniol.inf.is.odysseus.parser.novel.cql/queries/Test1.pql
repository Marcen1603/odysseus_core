pql_stream1 := ACCESS({Source='pql_stream2', Wrapper='GenericPush',
        Schema=[['timestamp ','Starttimestamp'],
                ['transaction_id','INTEGER'],
                ['name','String'],
                ['value','Integer']],
                transport='NonBlockingTcp',
                protocol='SizeByteBuffer',
                dataHandler='Tuple',
                Options=[['port','54321'], ['host','localhost']]})
                
select_1 = SELECT({predicate='value > 10'}, pql_stream1)               