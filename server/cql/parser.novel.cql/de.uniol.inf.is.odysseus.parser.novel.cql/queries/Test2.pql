pql_stream1 = ACCESS
(
	{
	source='pql_stream1',
	wrapper='GenericPush',
	transport='TCPClient',
	protocol='CSV',
	dataHandler='Tuple',
	options=[['host', 'example.com'],['port', '8080'],['read', '10240'],['write', '10240']],
	schema=[
				['id', 'INTEGER'],
				['first_name', 'String'],
				['last_name', 'String'],
				['email', 'String'],
				['gender', 'String'],
				['ip_adress', 'String']
			]
	}
)

select_1 = SELECT({
                predicate='id > 10'                                                        
              },
              pql_stream1
            )               