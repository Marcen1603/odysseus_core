#PARSER PQL
#ADDQUERY

input = ACCESS({source='Source',
wrapper='GenericPush',
transport='Timer',
protocol='None',
dataHandler='Tuple',
options=[['period', '1000']],
schema=[['time', 'Timestamp']]
})

data = MAP({EXPRESSIONS = [['rnd()','x'],['rnd()','y'],['rnd()','z']]}, input)

outputPy = Python({path='${ROOT}/Test.py'}, data)
outputRb = Ruby({path='${ROOT}/Test.rb'}, data)
output = Script({path='${ROOT}/Test.rb'}, data)