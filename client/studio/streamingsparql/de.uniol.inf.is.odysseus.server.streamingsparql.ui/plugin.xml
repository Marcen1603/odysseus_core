    <extension point="org.eclipse.ui.ide.markerResolution">
        <markerResolutionGenerator
            class="de.uniol.inf.is.odysseus.server.ui.StreamingsparqlExecutableExtensionFactory:org.eclipse.xtext.ui.editor.quickfix.MarkerResolutionGenerator"
            markerType="de.uniol.inf.is.odysseus.eca.ui.eca.check.fast">
            <attribute
                name="FIXABLE_KEY"
                value="true">
            </attribute>
        </markerResolutionGenerator>
        <markerResolutionGenerator
           class="de.uniol.inf.is.odysseus.server.ui.StreamingsparqlExecutableExtensionFactory:org.eclipse.xtext.ui.editor.quickfix.MarkerResolutionGenerator"
