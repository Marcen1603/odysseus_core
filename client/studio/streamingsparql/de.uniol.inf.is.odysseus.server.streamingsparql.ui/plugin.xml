<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<plugin
>

    <extension
            point="org.eclipse.ui.editors">
        <editor
            class="de.uniol.inf.is.odysseus.server.ui.StreamingsparqlExecutableExtensionFactory:org.eclipse.xtext.ui.editor.XtextEditor"
            contributorClass="org.eclipse.ui.editors.text.TextEditorActionContributor"
            default="true"
            extensions="sparql"
            id="de.uniol.inf.is.odysseus.server.Streamingsparql"
            name="Streamingsparql Editor">
        </editor>
    </extension>
    <extension
        point="org.eclipse.ui.handlers">
        <handler
            class="de.uniol.inf.is.odysseus.server.ui.StreamingsparqlExecutableExtensionFactory:org.eclipse.xtext.ui.editor.hyperlinking.OpenDeclarationHandler"
            commandId="org.eclipse.xtext.ui.editor.hyperlinking.OpenDeclaration">
            <activeWhen>
                <reference
                    definitionId="de.uniol.inf.is.odysseus.server.Streamingsparql.Editor.opened">
                </reference>
            </activeWhen>
        </handler>
        <handler
            class="de.uniol.inf.is.odysseus.server.ui.StreamingsparqlExecutableExtensionFactory:org.eclipse.xtext.ui.editor.handler.ValidateActionHandler"
            commandId="de.uniol.inf.is.odysseus.server.Streamingsparql.validate">
         <activeWhen>
            <reference
                    definitionId="de.uniol.inf.is.odysseus.server.Streamingsparql.Editor.opened">
            </reference>
         </activeWhen>
      	</handler>
      	<!-- copy qualified name -->
        <handler
            class="de.uniol.inf.is.odysseus.server.ui.StreamingsparqlExecutableExtensionFactory:org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedNameHandler"
            commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedName">
            <activeWhen>
				<reference definitionId="de.uniol.inf.is.odysseus.server.Streamingsparql.Editor.opened" />
            </activeWhen>
        </handler>
        <handler
            class="de.uniol.inf.is.odysseus.server.ui.StreamingsparqlExecutableExtensionFactory:org.eclipse.xtext.ui.editor.copyqualifiedname.OutlineCopyQualifiedNameHandler"
            commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.OutlineCopyQualifiedName">
            <activeWhen>
            	<and>
            		<reference definitionId="de.uniol.inf.is.odysseus.server.Streamingsparql.XtextEditor.opened" />
	                <iterate>
						<adapt type="org.eclipse.xtext.ui.editor.outline.IOutlineNode" />
					</iterate>
				</and>
            </activeWhen>
        </handler>
    </extension>
    <extension point="org.eclipse.core.expressions.definitions">
        <definition id="de.uniol.inf.is.odysseus.server.Streamingsparql.Editor.opened">
            <and>
                <reference definitionId="isActiveEditorAnInstanceOfXtextEditor"/>
                <with variable="activeEditor">
                    <test property="org.eclipse.xtext.ui.editor.XtextEditor.languageName" 
                        value="de.uniol.inf.is.odysseus.server.Streamingsparql" 
                        forcePluginActivation="true"/>
                </with>        
            </and>
        </definition>
        <definition id="de.uniol.inf.is.odysseus.server.Streamingsparql.XtextEditor.opened">
            <and>
                <reference definitionId="isXtextEditorActive"/>
                <with variable="activeEditor">
                    <test property="org.eclipse.xtext.ui.editor.XtextEditor.languageName" 
                        value="de.uniol.inf.is.odysseus.server.Streamingsparql" 
                        forcePluginActivation="true"/>
                </with>        
            </and>
        </definition>
    </extension>
    <extension
            point="org.eclipse.ui.preferencePages">
        <page
            class="de.uniol.inf.is.odysseus.server.ui.StreamingsparqlExecutableExtensionFactory:org.eclipse.xtext.ui.editor.preferences.LanguageRootPreferencePage"
            id="de.uniol.inf.is.odysseus.server.Streamingsparql"
            name="Streamingsparql">
            <keywordReference id="de.uniol.inf.is.odysseus.server.ui.keyword_Streamingsparql"/>
        </page>
        <page
            category="de.uniol.inf.is.odysseus.server.Streamingsparql"
            class="de.uniol.inf.is.odysseus.server.ui.StreamingsparqlExecutableExtensionFactory:org.eclipse.xtext.ui.editor.syntaxcoloring.SyntaxColoringPreferencePage"
            id="de.uniol.inf.is.odysseus.server.Streamingsparql.coloring"
            name="Syntax Coloring">
            <keywordReference id="de.uniol.inf.is.odysseus.server.ui.keyword_Streamingsparql"/>
        </page>
        <page
            category="de.uniol.inf.is.odysseus.server.Streamingsparql"
            class="de.uniol.inf.is.odysseus.server.ui.StreamingsparqlExecutableExtensionFactory:org.eclipse.xtext.ui.editor.templates.XtextTemplatePreferencePage"
            id="de.uniol.inf.is.odysseus.server.Streamingsparql.templates"
            name="Templates">
            <keywordReference id="de.uniol.inf.is.odysseus.server.ui.keyword_Streamingsparql"/>
        </page>
    </extension>
    <extension
            point="org.eclipse.ui.propertyPages">
        <page
            class="de.uniol.inf.is.odysseus.server.ui.StreamingsparqlExecutableExtensionFactory:org.eclipse.xtext.ui.editor.preferences.LanguageRootPreferencePage"
            id="de.uniol.inf.is.odysseus.server.Streamingsparql"
            name="Streamingsparql">
            <keywordReference id="de.uniol.inf.is.odysseus.server.ui.keyword_Streamingsparql"/>
            <enabledWhen>
	            <adapt type="org.eclipse.core.resources.IProject"/>
			</enabledWhen>
	        <filter name="projectNature" value="org.eclipse.xtext.ui.shared.xtextNature"/>
        </page>
    </extension>
    <extension
        point="org.eclipse.ui.keywords">
        <keyword
            id="de.uniol.inf.is.odysseus.server.ui.keyword_Streamingsparql"
            label="Streamingsparql"/>
    </extension>
    <extension
         point="org.eclipse.ui.commands">
      <command
            description="Trigger expensive validation"
            id="de.uniol.inf.is.odysseus.server.Streamingsparql.validate"
            name="Validate">
      </command>
      <!-- copy qualified name -->
      <command
            id="org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedName"
            categoryId="org.eclipse.ui.category.edit"
            description="Copy the qualified name for the selected element"
            name="Copy Qualified Name">
      </command>
      <command
            id="org.eclipse.xtext.ui.editor.copyqualifiedname.OutlineCopyQualifiedName"
            categoryId="org.eclipse.ui.category.edit"
            description="Copy the qualified name for the selected element"
            name="Copy Qualified Name">
      </command>
    </extension>
    <extension point="org.eclipse.ui.menus">
        <menuContribution
            locationURI="popup:#TextEditorContext?after=group.edit">
             <command
                 commandId="de.uniol.inf.is.odysseus.server.Streamingsparql.validate"
                 style="push"
                 tooltip="Trigger expensive validation">
            <visibleWhen checkEnabled="false">
                <reference
                    definitionId="de.uniol.inf.is.odysseus.server.Streamingsparql.Editor.opened">
                </reference>
            </visibleWhen>
         </command>  
         </menuContribution>
         <!-- copy qualified name -->
         <menuContribution locationURI="popup:#TextEditorContext?after=copy">
         	<command commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedName" 
         		style="push" tooltip="Copy Qualified Name">
            	<visibleWhen checkEnabled="false">
                	<reference definitionId="de.uniol.inf.is.odysseus.server.Streamingsparql.Editor.opened" />
            	</visibleWhen>
         	</command>  
         </menuContribution>
         <menuContribution locationURI="menu:edit?after=copy">
         	<command commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedName"
            	style="push" tooltip="Copy Qualified Name">
            	<visibleWhen checkEnabled="false">
                	<reference definitionId="de.uniol.inf.is.odysseus.server.Streamingsparql.Editor.opened" />
            	</visibleWhen>
         	</command>  
         </menuContribution>
         <menuContribution locationURI="popup:org.eclipse.xtext.ui.outline?after=additions">
			<command commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.OutlineCopyQualifiedName" 
				style="push" tooltip="Copy Qualified Name">
         		<visibleWhen checkEnabled="false">
	            	<and>
	            		<reference definitionId="de.uniol.inf.is.odysseus.server.Streamingsparql.XtextEditor.opened" />
						<iterate>
							<adapt type="org.eclipse.xtext.ui.editor.outline.IOutlineNode" />
						</iterate>
					</and>
				</visibleWhen>
			</command>
         </menuContribution>
    </extension>
    <extension point="org.eclipse.ui.menus">
		<menuContribution locationURI="popup:#TextEditorContext?endof=group.find">
			<command commandId="org.eclipse.xtext.ui.editor.FindReferences">
				<visibleWhen checkEnabled="false">
                	<reference definitionId="de.uniol.inf.is.odysseus.server.Streamingsparql.Editor.opened">
                	</reference>
            	</visibleWhen>
			</command>
		</menuContribution>
	</extension>
	<extension point="org.eclipse.ui.handlers">
	    <handler
            class="de.uniol.inf.is.odysseus.server.ui.StreamingsparqlExecutableExtensionFactory:org.eclipse.xtext.ui.editor.findrefs.FindReferencesHandler"
            commandId="org.eclipse.xtext.ui.editor.FindReferences">
            <activeWhen>
                <reference
                    definitionId="de.uniol.inf.is.odysseus.server.Streamingsparql.Editor.opened">
                </reference>
            </activeWhen>
        </handler>
    </extension>   

<!-- adding resource factories -->

