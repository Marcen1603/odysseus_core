            class="de.uniol.inf.is.odysseus.server.ui.StreamingsparqlExecutableExtensionFactory:org.eclipse.xtext.builder.preferences.BuilderPreferencePage"
            id="de.uniol.inf.is.odysseus.server.Streamingsparql.compiler.propertyPage"
            name="Compiler">
            <keywordReference id="de.uniol.inf.is.odysseus.server.ui.keyword_Streamingsparql"/>
