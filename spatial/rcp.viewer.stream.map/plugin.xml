<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="de.uniol.inf.is.odysseus.rcp.viewer.stream.map.activator.StreamMapEditorEarlyStartup">
      </startup>
   </extension>
   <extension
         point="de.uniol.inf.is.odysseus.rcp.viewer.StreamEditor">
      <editor
            class="de.uniol.inf.is.odysseus.rcp.viewer.stream.map.StreamMap10Editor"
            id="de.uniol.inf.is.odysseus.rcp.viewer.stream.map.StreamMap10Editor"
            label="Map 10">
      </editor>
      <editor
            class="de.uniol.inf.is.odysseus.rcp.viewer.stream.map.StreamMap10000Editor"
            id="de.uniol.inf.is.odysseus.rcp.viewer.stream.map.StreamMap10000Editor"
            label="Map 10000">
      </editor>
      <editor
            class="de.uniol.inf.is.odysseus.rcp.viewer.stream.map.StreamMapWindowEditor"
            id="de.uniol.inf.is.odysseus.rcp.viewer.stream.map.StreamMapWindowEditor"
            label="Map Window">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="de.uniol.inf.is.odysseus.rcp.viewer.stream.map.commands.AddLayerCommand"
            id="de.uniol.inf.is.odysseus.rcp.viewer.stream.map.commands.AddLayerCommand"
            name="Add Map Layer">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.views">
               <view
            allowMultiple="false"
            category="Odysseus"
            class="de.uniol.inf.is.odysseus.rcp.viewer.stream.map.views.MapLayerView"
            id="de.uniol.inf.is.odysseus.rcp.views.MapLayerView"
            name="MapLayer View"
            restorable="true">
      </view>
         
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
   </extension>
   
   <extension point="org.eclipse.ui.menus">
 			<menuContribution locationURI="menu:org.eclipse.ui.main.menu?after=additions">
	 			<menu
	               label="Geo Information"
	               id="org.eclipse.ui.examples.contributions.menus.globalMenu">
	         		<command 
	         			commandId="de.uniol.inf.is.odysseus.rcp.viewer.stream.map.commands.AddLayerCommand" 
	         			label="Add Map Layer" 
	         			style="push">
	         		</command>
	         		<separator
	                  name="additions"
	                  visible="true">
	            	</separator>
	         	</menu>
      		</menuContribution>
   </extension>
   
   
</plugin>
