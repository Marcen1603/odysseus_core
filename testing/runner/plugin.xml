<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="de.uniol.inf.is.odysseus.test.runner.application"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="de.uniol.inf.is.odysseus.test.runner.Tester">
         </run>
      </application>
   </extension>
   <extension
         name="Odysseus Test"
         point="org.eclipse.core.runtime.products">
      <product
            application="de.uniol.inf.is.odysseus.test.runner.Tester"
            name="Odysseus Test">
      </product>
   </extension>
   <extension
         id="OdysseusTest"
         point="org.eclipse.core.runtime.products">
      <product
            application="de.uniol.inf.is.odysseus.test.runner.application"
            name="Odysseus Testing">
         <property
               name="appName"
               value="Odysseus Testing">
         </property>
      </product>
   </extension>

</plugin>
