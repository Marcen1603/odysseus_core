<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="de.uniol.inf.is.odysseus.rcp.evaluation.execution.StartEvaluationCommand"
            id="de.uniol.inf.is.odysseus.rcp.evaluation.start"
            name="Start Evaluation Command">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="true"
            locationURI="toolbar:org.eclipse.ui.main.toolbar">
         <command
               commandId="de.uniol.inf.is.odysseus.rcp.evaluation.start"
               icon="icons/application-run.png"
               label="Start Evaluation"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.ui.navigator.ProjectExplorer#PopupMenu?after=additions">
         <command
               commandId="de.uniol.inf.is.odysseus.rcp.evaluation.start"
               icon="icons/application-run.png"
               label="Start Evaluation"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="true"
            locationURI="popup:#EditorContext?after=additions">
         <command
               commandId="de.uniol.inf.is.odysseus.rcp.evaluation.start"
               icon="icons/application-run.png"
               label="Start Evaluation"
               style="push">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="de.uniol.inf.is.odysseus.rcp.evaluation.editor.EvaluationEditorPart"
            default="true"
            extensions="eval"
            icon="icons/application-run.png"
            id="de.uniol.inf.is.odysseus.rcp.evaluation.editor.EvaluationEditorPart"
            name="Evaluation Job Editor">
      </editor> </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <wizard
            category="de.uniol.inf.is.odysseus.rcp.wizard.category"
            class="de.uniol.inf.is.odysseus.rcp.evaluation.wizards.NewEvaluationWizard"
            icon="icons/application-run.png"
            id="de.uniol.inf.is.odysseus.rcp.evaluation.newfilewizard"
            name="Evaluation Job"
            project="false">
      </wizard>
   </extension>
    <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="*">
         <newWizardShortcut
               id="de.uniol.inf.is.odysseus.rcp.evaluation.newfilewizard">
         </newWizardShortcut>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.ui.navigator.navigatorContent">
      <commonWizard
            type="new"
            wizardId="de.uniol.inf.is.odysseus.rcp.evaluation.newfilewizard">
         <enablement></enablement>
      </commonWizard>
   </extension>

</plugin>
