<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="de.uniol.inf.is.odysseus.rcp.DashboardPart">
      <dashboardPart
            class="de.uniol.inf.is.odysseus.rcp.dashboard.part.SampleDashboardPart"
            description="A sample for creating dashboard parts"
            name="Sample">
         <setting
               defaultValue="Default"
               description="Sample setting in string type"
               isEditable="true"
               isOptional="false"
               name="StringSetting"
               type="String">
         </setting>
         <setting
               defaultValue="42"
               description="Some sample setting in integer type"
               isEditable="true"
               isOptional="false"
               name="IntegerSetting"
               type="Integer">
         </setting>
      </dashboardPart>
      <dashboardPart
            class="de.uniol.inf.is.odysseus.rcp.dashboard.part.TableDashboardPart"
            description="Tabular overview of recieved tuples of one query"
            name="Table">
         <setting
               description="Comma-separated list of attribute-names shown in the table. Must be not empty."
               isEditable="true"
               isOptional="false"
               name="Attributes"
               type="String">
         </setting>
      </dashboardPart>
   </extension>

</plugin>
