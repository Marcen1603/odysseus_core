            name="ACCESS">
         <editor
               class="de.uniol.inf.is.odysseus.rcp.editor.parameters.SourceParameterEditor"
               name="SOURCE">
         </editor>
         <editor
               class="de.uniol.inf.is.odysseus.rcp.editor.parameters.StringParameterEditor"
               name="TYPE">
         </editor>
         <editor
               class="de.uniol.inf.is.odysseus.rcp.editor.parameters.LongParameterEditor"
