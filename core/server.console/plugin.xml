<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="OdysseusServer"
         name="OdysseusServer"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="de.uniol.inf.is.odysseus.core.server.console.OdysseusServerApplication">
         </run>
      </application>
   </extension>
   <extension
         id="OdysseusServer"
         point="org.eclipse.core.runtime.products">
      <product
            application="de.uniol.inf.is.odysseus.core.server.console.OdysseusServer"
            name="Odysseus Server">
         <property
               name="appName"
               value="Odysseus Server">
         </property>
      </product>
   </extension>

</plugin>
