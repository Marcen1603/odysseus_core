-query "LOGICAL PLAN : 
PROJECTION(
	SELECTION(
		JOIN(
			SET-PREDICTION(
				SLIDING-TIME-WINDOW(
					ACCESS(movingObjectsOL AS a),
					4,
					1,
					a.timestamp
				),
				a.x := a.x + 1/sqrt((a.xNext - a.x)^2 + (a.yNext - a.y)^2) * (a.xNext - a.x) * a.speed;
a.y := a.y + 1/sqrt((a.xNext - a.x)^2 + (a.yNext - a.y)^2) * (a.yNext - a.y) * a.speed; WHERE a.id > 0.0;
			),
			SET-PREDICTION(
				SLIDING-TIME-WINDOW(
					ACCESS(movingObjectsOL AS b),
					1,
					1,
					b.timestamp
				),
				b.x := b.x + 1/sqrt((b.xNext - b.x)^2 + (b.yNext - b.y)^2) * (b.xNext - b.x) * b.speed ;
				b.y := b.y +1/sqrt((b.xNext - b.x)^2 + (b.yNext - b.y)^2) * (b.yNext - b.y) * b.speed ; WHERE b.id > 0.0 ;
			)
		),			
		sqrt((a.x - b.x)^2 + (a.y -b.y)^2) < 6
	),
	a.type a.id a.sequenceNo a.objectType a.timestamp a.x a.y a.speed a.xNext a.yNext b.type b.id b.sequenceNo b.objectType b.timestamp b.x b.y b.speed b.xNext b.yNext
)"
-schedstrat "roundrobin"
-lang "procedural"
-useMV
-bufferPlacement no
-usePrediction