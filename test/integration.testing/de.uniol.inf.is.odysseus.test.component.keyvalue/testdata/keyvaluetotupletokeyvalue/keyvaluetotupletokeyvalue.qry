#PARSER PQL
#DROPALLSOURCES
#DROPALLQUERIES
#ADDQUERY
percentage = ACCESS({
	source='percentage', 
	wrapper='GenericPull',
	transport='file',
	protocol='json',
	dataHandler='KeyValueObject', 
	options=[['filename', '${BUNDLE-ROOT}\testdata\keyvaluetotupletokeyvalue\input0.json']],
	schema=[['id','STARTTIMESTAMP']]
})

keyvaluetotuple0 = KEYVALUETOTUPLE({schema=[['id','Integer'],['percentage','Integer']], KEEPINPUT = 'false', TYPE = 'type'}, percentage)

tupletokeyvalue0 = TUPLETOKEYVALUE({type='KEYVALUEOBJECT'}, keyvaluetotuple0)

