///OdysseusScript
#PARSER PQL
#DROPALLSOURCES
#DROPALLQUERIES
#ADDQUERY
percentage = ACCESS({
	source='percentage', 
	wrapper='GenericPull',
	transport='file',
	protocol='json',
	dataHandler='KeyValueObject', 
	options=[['filename', '${BUNDLE-ROOT}\testdata\keyvaluetotupletimestamp\input0.json']]
})

keyvaluetotuple0 = KEYVALUETOTUPLE({schema=[['id','STARTTIMESTAMP'],['percentage','Integer']], KEEPINPUT = 'false', TYPE = 'type'}, percentage)

longToInt = MAP({EXPRESSIONS=[['toInteger(id)','id'],['percentage','percentage']]}, keyvaluetotuple0)

tupletokeyvalue0 = TUPLETOKEYVALUE(longToInt)

