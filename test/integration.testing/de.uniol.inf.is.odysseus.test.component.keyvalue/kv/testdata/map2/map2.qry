///OdysseusScript
#PARSER PQL
#DROPALLSOURCES
#DROPALLQUERIES
#ADDQUERY
json2 = ACCESS({
	source='json', 
	wrapper='GenericPull',
	transport='File',
	protocol='JSON',
	dataHandler='KeyValueObject',
	options=[['filename','${BUNDLE-ROOT}\testdata\map2\input.json']],
	schema=[['timestamp','STARTTIMESTAMP']]
})
mapped2 = MAP({expressions = [['customer.id', 'kaeufer.id']
							],
				keepAllAttributes = 'true',
				removeAttributes = ['customer.email', 'customer.ip_address']}, json2)

