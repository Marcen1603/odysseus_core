///OdysseusScript
#PARSER PQL
#DROPALLSOURCES
#DROPALLQUERIES
#ADDQUERY
percentage = ACCESS({
	source='percentage', 
	wrapper='GenericPull',
	transport='file', 
	protocol='json',
	dataHandler='KeyValueObject', 
	options=[['filename', '${BUNDLE-ROOT}\testdata\select\input0.json']],
	schema=[['id','STARTTIMESTAMP']]
})
select0 = select({predicate = 'percentage >= 50'}, percentage)

