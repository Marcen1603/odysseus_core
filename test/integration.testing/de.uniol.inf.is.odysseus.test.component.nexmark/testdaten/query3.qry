///OdysseusScript
#PARSER CQL
#TRANSCFG Standard
#RUNQUERY
DROP STREAM nexmark:bid2;
#RUNQUERY
ATTACH STREAM nexmark:bid2 (timestamp STARTTIMESTAMP,	auction INTEGER, bidder INTEGER, datetime LONG,	price DOUBLE) FILE 'nexmark/daten/nexmark_bid2.csv' AS SimpleCSV;

#RUNQUERY
SELECT b.auction, DolToEur(b.price) AS euroPrice, b.bidder, b.datetime FROM nexmark:bid2 [UNBOUNDED] AS b;
