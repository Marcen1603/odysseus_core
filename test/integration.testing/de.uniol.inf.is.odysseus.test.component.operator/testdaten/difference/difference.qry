///OdysseusScript
///#DEFINE PATH ${BUNDLE-ROOT}\testdaten\difference
#DEFINE PATH E:/odysseus/test/integration.testing/de.uniol.inf.is.odysseus.test.component.operator/testdaten/difference/

#PARSER PQL
#ADDQUERY
wind0 = ACCESS({
	source='wind0', 
	wrapper='GenericPull',
	transport='file', 
	protocol='SimpleCSV',
	dataHandler='Tuple', 
	options=[
		['filename', '${PATH}\input0.csv'],
		['csv.delimiter', ';'],
		['csv.trim', 'true'],
		['delay', '200']
	], 
	schema=[
		['timestamp', 'STARTTIMESTAMP'], 
		['speed', 'INTEGER'], 
		['direction', 'STRING'], 
		['name', 'STRING'],
		['location', 'INTEGER']
]})
wind1 = ACCESS({
	source='wind1', 
	wrapper='GenericPull',
	transport='file', 
	protocol='SimpleCSV',
	dataHandler='Tuple', 
	options=[
		['filename', '${PATH}\input1.csv'],
		['csv.delimiter', ';'],
		['csv.trim', 'true']
	], 
	schema=[
		['timestamp', 'STARTTIMESTAMP'], 
		['speed', 'INTEGER'], 
		['direction', 'STRING'], 
		['name', 'STRING'],
		['location', 'INTEGER']
]})
window_wind0 = timeWindow({size = [5, 'SECONDS'], slide = [5, 'SECONDS']}, wind0)
window_wind1 = timeWindow({size = [5, 'SECONDS'], slide = [5, 'SECONDS']}, wind1)

result = DIFFERENCE(window_wind0, window_wind1)