///OdysseusScript
#PARSER PQL
#TRANSCFG Standard
#RUNQUERY
percentage = ACCESS({
	source='percentage', 
	wrapper='GenericPull',
	transport='file', 
	protocol='SimpleCSV',
	dataHandler='Tuple', 
	options=[
		['filename', '$input0'], 
		['csv.delimiter', ';'],
		['csv.trim', 'true']
		], 
	schema=[
		['timestamp', 'STARTTIMESTAMP'], 
		['percentage', 'INTEGER']
]})
window0 = window({size = 5, advance = 1, type = 'tuple'}, percentage)
aggregate0 = aggregate({aggregations=[['AVG', 'percentage', 'avgPerc', 'long']]}, window0)
file0 = filesink({file='$output0'}, aggregate0)