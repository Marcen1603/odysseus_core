random2_ = CSVFILESOURCE({SCHEMA = [
		['key2', 'Long'],
		['value2', 'String'],
		['startts', 'StartTimestamp'],
		['endts', 'EndTimestamp']
	], SOURCE = 'random2', FILENAME = '${BUNDLE-ROOT}/testdaten/join_hashjoinsa/random2.csv'})
random2 = PROJECT({ATTRIBUTES = ['key2', 'value2']}, random2_)

joined = JOIN({PREDICATE = 'key1 = key2', SWEEPAREANAME = 'HashJoinSA'}, random1, random2)
