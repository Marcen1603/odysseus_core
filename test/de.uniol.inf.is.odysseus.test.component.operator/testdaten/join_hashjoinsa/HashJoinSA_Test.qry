random1_ = CSVFILESOURCE({SCHEMA = [
		['key1', 'Long'],
		['value1', 'String'],
		['startts', 'StartTimestamp'],
		['endts', 'EndTimestamp']
	], SOURCE = 'random1', FILENAME = '${BUNDLE-ROOT}/testdaten/join_hashjoinsa/random1.csv'})
