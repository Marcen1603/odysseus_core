<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>  
<extension
         point="org.eclipse.ui.editors">
      <editor
            class="de.uniol.inf.is.odysseus.rcp.test.editor.TestEditorPart"
            default="true"
            extensions="case"
            icon="icons/application-run.png"
            id="de.uniol.inf.is.odysseus.rcp.test.editor.TestEditorPart"
            name="Test Case Editor">
      </editor> </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <wizard
            category="de.uniol.inf.is.odysseus.rcp.wizard.category"
            class="de.uniol.inf.is.odysseus.rcp.test.wizard.TestWizard"
            icon="icons/application-run.png"
            id="de.uniol.inf.is.odysseus.rcp.test.testwizard"
            name="Test Case"
            project="false">
      </wizard>
   </extension>
    <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="*">
         <newWizardShortcut
               id="de.uniol.inf.is.odysseus.rcp.test.testwizard">
         </newWizardShortcut>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.ui.navigator.navigatorContent">
      <commonWizard
            type="new"
            wizardId="de.uniol.inf.is.odysseus.rcp.test.testwizard">
         <enablement></enablement>
      </commonWizard>
   </extension>
</plugin>