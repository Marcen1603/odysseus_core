<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            category="Odysseus"
            class="de.uniol.inf.is.odysseus.rcp.p2p_new.views.PeerView"
            icon="icons/peer.png"
            id="de.uniol.inf.is.odysseus.rcp.p2p_new.PeerView"
            name="Peer View"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="de.uniol.inf.is.odysseus.rcp.p2p_new.commands.RefreshPeerViewCommand"
            id="de.uniol.inf.is.odysseus.rcp.commands.RefreshPeerViewCommand"
            name="Refresh">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="toolbar:de.uniol.inf.is.odysseus.rcp.p2p_new.PeerView">
         <command
               commandId="de.uniol.inf.is.odysseus.rcp.commands.RefreshPeerViewCommand"
               icon="icons/refresh.gif"
               label="Refresh"
               style="push"
               tooltip="Refresh">
         </command>
      </menuContribution>
   </extension>

</plugin>
