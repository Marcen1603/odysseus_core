<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            allowMultiple="false"
            class="de.uniol.inf.is.odysseus.rcp.viewer.view.views.ModelManagerViewPart"
            id="de.uniol.inf.is.odysseus.rcp.viewer.view.ModelManagerView"
            name="ModelManager View"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="de.uniol.inf.is.odysseus.rcp.application.perspective.viewer">
         <view
               closeable="true"
               id="de.uniol.inf.is.odysseus.rcp.viewer.view.ModelManagerView"
               minimized="false"
               moveable="true"
               ratio="0.95f"
               relationship="stack"
               relative="org.eclipse.ui.console.ConsoleView"
               showTitle="true"
               standalone="false"
               visible="true">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="de.uniol.inf.is.odysseus.rcp.viewer.view.editor.GraphViewEditor"
            default="false"
            id="de.uniol.inf.is.odysseus.rcp.viewer.view.GraphEditor"
            name="Graph Editor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="de.uniol.inf.is.odysseus.rcp.viewer.view.commands.CallGraphEditor"
            id="de.uniol.inf.is.odysseus.rcp.viewer.view.commands.callGraphEditor"
            name="Call Graph Editor">
      </command>
      <command
            defaultHandler="de.uniol.inf.is.odysseus.rcp.viewer.view.commands.AddQuery"
            id="de.uniol.inf.is.odysseus.rcp.viewer.view.commands.addQuery"
            name="Add Query">
      </command>
      <command
            defaultHandler="de.uniol.inf.is.odysseus.rcp.viewer.view.commands.ShowQueryDialog"
            id="de.uniol.inf.is.odysseus.rcp.viewer.view.commands.showQueryDialog"
            name="Query Dialog">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu">
         <menu
               id="osgiMenu"
               label="OSGi-Commands">
            <command
                  commandId="de.uniol.inf.is.odysseus.rcp.viewer.view.commands.showQueryDialog"
                  label="Add Query..."
                  style="push">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar">
         <toolbar
               id="de.uniol.inf.is.odysseus.rcp.viewer.view.toolbar1">
            <command
                  commandId="de.uniol.inf.is.odysseus.rcp.viewer.view.commands.showQueryDialog"
                  label="Add Query..."
                  style="push">
            </command>
         </toolbar>
      </menuContribution>
   </extension>


</plugin>
